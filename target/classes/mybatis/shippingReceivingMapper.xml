<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sr">
	<select id="selectAll" resultType="com.got.vo.ShippingReceivingVO">
		SELECT * FROM shipping_receiving
	</select>
	
	<select id="selectWithG_no" resultType="com.got.vo.ShippingReceivingVO">
		SELECT r.* FROM (SELECT * FROM shipping_receiving WHERE g_no = #{g_no} ORDER BY regdate) r
		<if test="count != null">
			<![CDATA[WHERE ROWNUM <= #{count}]]>
		</if>
	</select>
	
</mapper>